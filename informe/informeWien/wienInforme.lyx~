\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula $\frac{C_{serie}R_{serie}-R_{p}(C_{serie}+C_{paralelo})}{C_{serie}R_{serie}+R_{p}(C_{serie}+C_{paralelo})}$
\end_inset


\end_layout

\end_inset
</cell>
</row>
<row>
<cell alignment="center" valignment="top" topline="true" leftline="true" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula $R_{serie}$
\end_inset


\end_layout

\end_inset
</cell>
<cell alignment="center" valignment="top" topline="true" leftline="true" rightline="true" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula $\frac{-C_{serie}R_{serie}+R_{p}(C_{serie}+C_{paralelo})}{C_{serie}R_{serie}+R_{p}(C_{serie}+C_{paralelo})}$
\end_inset


\end_layout

\end_inset
</cell>
</row>
<row>
<cell alignment="center" valignment="top" topline="true" leftline="true" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula $C_{paralelo}$
\end_inset


\end_layout

\end_inset
</cell>
<cell alignment="center" valignment="top" topline="true" leftline="true" rightline="true" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula $\frac{C_{serie}(R_{paralelo}+R_{serie})-C_{paralelo}R_{paralelo}}{C_{serie}(R_{paralelo}+R_{serie})+C_{paralelo}R_{paralelo}}$
\end_inset


\end_layout

\end_inset
</cell>
</row>
<row>
<cell alignment="center" valignment="top" topline="true" bottomline="true" leftline="true" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula $C_{serie}$
\end_inset


\end_layout

\end_inset
</cell>
<cell alignment="center" valignment="top" topline="true" bottomline="true" leftline="true" rightline="true" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula $\frac{-C_{serie}(R_{paralelo}+R_{serie})+C_{paralelo}R_{paralelo}}{C_{serie}(R_{paralelo}+R_{serie})+C_{paralelo}R_{paralelo}}$
\end_inset


\end_layout

\end_inset
</cell>
</row>
</lyxtabular>

\end_inset


